{
  "name": "depositify",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "start:backend": "turbo start:dev",
    "start:frontend": "turbo dev",
    "start:all": "turbo start:all",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.1.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.3",
  "workspaces": [
    "frontend/*",
    "backend/*",
    "packages/*"
  ],
  "volta": {
    "node": "22.9.0"
  }
}
